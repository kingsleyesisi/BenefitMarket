{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.6.1"
    }
  },
  "rewrites": [
    { "source": "/(.*\\.php)", "destination": "/$1" },
    { "source": "/(.*\\\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|otf|webp))", "destination": "/$1" },
    { "source": "/(.*)", "destination": "/index.php" }
  ]
}
